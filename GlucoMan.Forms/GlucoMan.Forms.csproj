<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>Current_Icon_White.ico</ApplicationIcon>
    <PackageLicenseExpression>GPL-2.0-only</PackageLicenseExpression>
    <AssemblyVersion>0.7.4</AssemblyVersion>
    <FileVersion>0.7.4</FileVersion>
    <NeutralLanguage>en-001</NeutralLanguage>
    <PackageIcon>gamon LegoLogo scritta con piastra bianca 256x256.png</PackageIcon>
    <Authors>Ing.Gabriele Monti - Forlì - Italia</Authors>
    <Version>0.7.3.4</Version>
    <PackageId>GlucoMan</PackageId>
    <AssemblyName>GlucoMan</AssemblyName>
    <AutoGenerateBindingRedirects>false</AutoGenerateBindingRedirects>
    <StartupObject>GlucoMan.Forms.Main_GlucoMan_Forms</StartupObject>
  </PropertyGroup>

  <Import Project="..\SharedGeneral\SharedGeneral.projitems" Label="Shared" />

  <Import Project="..\SharedGlucoMan\SharedGlucoMan.projitems" Label="Shared" />

  <Import Project="..\SharedWindows\SharedWindows.projitems" Label="Shared" />

  <Import Project="..\SharedWinForms\SharedWinForms.projitems" Label="Shared" />

  <ItemGroup>
    <Compile Remove="frmGlucose - Copia.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="frmGlucose.Designer.cs.OLD" />
    <None Remove="frmGlucose.resx.OLD" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Current_Icon_White.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="8.0.7" />
    <PackageReference Include="SQLitePCLRaw.bundle_e_sqlite3" Version="2.1.8" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>